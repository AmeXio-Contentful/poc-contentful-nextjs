BUNDLE_ANALYZE=false
ENVIRONMENT_NAME=local

CONFIG_CONTENTFUL_META_URL=http://localhost:3000

# Source Environment (Where you want to source the template from e.g. The Growth master template)
NEXT_PUBLIC_CONFIG_CONTENTFUL_MAIN_SPACE_ID=
