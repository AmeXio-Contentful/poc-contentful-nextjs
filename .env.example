NEXT_PUBLIC_CONFIG_CONTENTFUL_MAIN_SPACE_PREVIEW_TOKEN=

# Your personal access token: https://app.contentful.com/account/profile/cma_tokens
CONFIG_CONTENTFUL_MAIN_SPACE_MANAGEMENT_TOKEN=

