  "engines": {
    "node": "16.16.0",
    "npm": "Please use Yarn"
  },
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "next start",
    "dev": "next dev",
    "dev:cd": "NODE_OPTIONS='--inspect' circularDependencies=true next dev",
    "build": "next build",
    "export": "next build && next export",
    "prepare": "husky install",
    "deploy": "node scripts/provisioning-scripts",
    "type-check": "tsc --noEmit",
    "test": "echo \"Error: no test specified\" && exit 1",
    "migrate:init": "ctf-migrate init",
    "migrate:content-model": "ctf-migrate up -a",
    "lint": "eslint './src/**/*.{js,jsx,ts,tsx}'",
    "lint-fix": "yarn lint --fix",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "graphql-codegen:generate": "graphql-codegen -r dotenv/config --config codegen.ts",
    "graphql-codegen:watch": "graphql-codegen --watch -r dotenv/config --config codegen.ts",
    "contentful-scripts:provision": "ts-node scripts/provision.ts --project ./tsconfig.json",
    "contentful-scripts:delete": "ts-node scripts/delete.ts --project ./tsconfig.json"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --quiet --fix"
    ],
    "src/**/*.{json,js,ts,jsx,tsx}": [
      "prettier --write"
    ]
  },
